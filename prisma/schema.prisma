
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Guest {
  id            String       @default(cuid()) @id
  first         String
  last          String
  attending     String
  email         String       @unique
  meal          String?
  kids          String?
}
